                            <StackPanel>
                                <Label Content="Oceatch" Foreground="White" HorizontalAlignment="Center"/>
                                <cp:Calendar Width="220" Height="220" Margin="5,0" IsTodayHighlighted="True" Theme="Oceatech" />
                            </StackPanel>
                            <StackPanel>
                                <Label Content="Contactel" Foreground="White" HorizontalAlignment="Center"/>
                                <cp:Calendar Width="220" Height="220" Margin="5,0" IsTodayHighlighted="True" Theme="Contactel">
                                    
