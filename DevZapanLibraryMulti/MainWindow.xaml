            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="437*"/>
            <ColumnDefinition Width="217*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <z:ZapTabControl x:Name="tabControl" 
                         Grid.ColumnSpan="3" 
                         Grid.RowSpan="2" 
                         Theme="Oceatech" 
                         TabAdd="ZapTabControl_TabAdd" 
                         CanAddItem="True" 
                         Background="#505050">
            <z:ZapTabControl.ContextMenu>
                <z:ZapContextMenu Theme="Contactel" StaysOpen="True">
                    <z:ZapMenuItem Header="Submenus">
                        <z:ZapMenuItem Header="Submenus level 1">
                            <z:ZapMenuItem Header="Submenus level 2">
                                <z:ZapMenuItem Header="Submenus level 3" />
                            </z:ZapMenuItem>
                        </z:ZapMenuItem>
                    </z:ZapMenuItem>
                    <z:ZapMenuItem Header="Test" />
                    <z:ZapMenuItem Header="Test" />
                    <z:ZapMenuItem Header="Test" />
                </z:ZapContextMenu>
            </z:ZapTabControl.ContextMenu>

            <z:ZapTabItem Header="DatePicker" IsClosable="False">
                <StackPanel>
                    <dp:ZapDatePicker Grid.Column="0" 
                                      HorizontalAlignment="Left" 
                                      Margin="5"
                                      Theme="Oceatech" 
                                      IsCheckable="True" 
                                      IsReadOnly="False"
                                      CalendarPlacement="Left"
                                      />
                    <dp:ZapDatePicker Grid.Column="1" 
                                      HorizontalAlignment="Left" 
                                      Margin="5"
                                      Theme="Contactel" 
                                      ButtonStyle="Brush"
                                      CalendarPlacement="Right"
                                      />

                    <TextBox HorizontalAlignment="Left" Text="test" />

                    <z:ZapButton Content="test" HorizontalAlignment="Left" />
                </StackPanel>
            </z:ZapTabItem>
            <z:ZapTabItem Header="ListView" IsClosable="False">
                <z:ListView ItemsSource="{Binding ListViewDatas, IsAsync=True}" ItemDoubleClick="ListView_ItemDoubleClick" Margin="-3,0">
                    <z:ListView.View>
                        <z:GridView>
                            <z:GridViewColumn Header="Id" DisplayMemberBinding="{Binding Id}" SortDirection="Descending" SearchType="Text" />
                            <z:GridViewColumn Header="FirstName" DisplayMemberBinding="{Binding FirstName}" SearchType="Text" />
                            <z:GridViewColumn Header="LastName" DisplayMemberBinding="{Binding LastName}" SearchType="Text" />
                            <z:GridViewColumn Header="DateTime" DisplayMemberBinding="{Binding DateTime, StringFormat={}{0:dd/MM/yyyy HH:mm:ss}}" SearchType="Text" />
                            <z:GridViewColumn Header="IsActive" DisplayMemberBinding="{Binding IsActrive}" SearchType="Text" />
                            <z:GridViewColumn Header="Text" DisplayMemberBinding="{Binding Text}" SearchType="Text" />
                        </z:GridView>
                    </z:ListView.View>
