            </z:ZapTabItem>
            <z:ZapTabItem Header="LoadingIndicator" IsClosable="False">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Row="0" Grid.Column="1" Text="Oceatech" Margin="10,0" />
                    <TextBlock Grid.Row="0" Grid.Column="2" Text="Contactel" Margin="10,0" />

                    <TextBlock Grid.Row="1" Grid.Column="0" Text="Arcs" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="2" Grid.Column="0" Text="ArcsRing" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="3" Grid.Column="0" Text="DoubleBounce" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="4" Grid.Column="0" Text="FlipPane" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="5" Grid.Column="0" Text="Pulse" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="6" Grid.Column="0" Text="Ring" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="7" Grid.Column="0" Text="ThreeDots" HorizontalAlignment="Right" Margin="10" />
                    <TextBlock Grid.Row="8" Grid.Column="0" Text="Wave" HorizontalAlignment="Right" Margin="10" />

                    <z:ZapLoadingIndicator Grid.Row="1" Grid.Column="1" ZapTemplate="Arcs" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="2" Grid.Column="1" ZapTemplate="ArcsRing" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="3" Grid.Column="1" ZapTemplate="DoubleBounce" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="4" Grid.Column="1" ZapTemplate="FlipPane" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="5" Grid.Column="1" ZapTemplate="Pulse" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="6" Grid.Column="1" ZapTemplate="Ring" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="7" Grid.Column="1" ZapTemplate="ThreeDots" Width="40" />
                    <z:ZapLoadingIndicator Grid.Row="8" Grid.Column="1" ZapTemplate="Wave" Height="25" />

                    <z:ZapLoadingIndicator Grid.Row="1" Grid.Column="2" Theme="Contactel" ZapTemplate="Arcs" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="2" Grid.Column="2" Theme="Contactel" ZapTemplate="ArcsRing" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="3" Grid.Column="2" Theme="Contactel" ZapTemplate="DoubleBounce" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="4" Grid.Column="2" Theme="Contactel" ZapTemplate="FlipPane" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="5" Grid.Column="2" Theme="Contactel" ZapTemplate="Pulse" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="6" Grid.Column="2" Theme="Contactel" ZapTemplate="Ring" Width="20" Height="20" />
                    <z:ZapLoadingIndicator Grid.Row="7" Grid.Column="2" Theme="Contactel" ZapTemplate="ThreeDots" Width="40" />
                    <z:ZapLoadingIndicator Grid.Row="8" Grid.Column="2" Theme="Contactel" ZapTemplate="Wave" Height="25" />

                </Grid>
            </z:ZapTabItem>
            <z:ZapTabItem Header="Progress" IsClosable="False">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <z:ZapProgress x:Name="progressFlat" Grid.Column="0" Grid.Row="0" Value="25" IsIndeterminate="false" Text="Oceatech Flat" />
                    <z:ZapProgress x:Name="progressGlass" Grid.Column="0" Grid.Row="1" Value="75" ZapTemplate="Glass" IsIndeterminate="false" Text="Oceatech Glass" />
                    <z:ZapProgress x:Name="progressIndeterminate" Grid.Column="0" Grid.Row="2" Value="00" ZapTemplate="Glass" IsIndeterminate="True" Text="Oceatech Indeterminate" />
                    <z:ZapProgress x:Name="progressFlat1" Grid.Column="1" Grid.Row="0" Value="40" IsIndeterminate="false" Theme="Contactel" Text="Contactel Flat" />
                    <z:ZapProgress x:Name="progressGlass1" Grid.Column="1" Grid.Row="1" Value="100" ZapTemplate="Glass" IsIndeterminate="false" Theme="Contactel" Text="Contactel Glass" />
                    <z:ZapProgress x:Name="progressIndeterminate1" Grid.Column="1" Grid.Row="2" Value="50" ZapTemplate="Glass" IsIndeterminate="True" Theme="Contactel" Text="Contactel Indeterminate" />

                </Grid>
            </z:ZapTabItem>
