<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:r="clr-namespace:ZapanControls.Controls.ResourceKeys"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- ================= -->
    <!-- = Oceatech Keys = -->
    <!-- ================= -->

    <!--control-->
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=Background}" Color="#FF505050" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=BorderBrush}" Color="Indigo" />
    <Thickness x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=BorderThickness}">1</Thickness>
    
    <!--titlebar-->
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=TitleBarBackground}" Color="DarkViolet" />
    <SolidColorBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=TitleBarForeground}" Color="White" />
    <sys:Double x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type r:ZapWindowResourceKeys}, ResourceId=TitleBarHeight}">30.0</sys:Double>

</ResourceDictionary>